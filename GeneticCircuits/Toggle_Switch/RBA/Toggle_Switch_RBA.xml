          <addedPrefix:speciesReference species="IPTG" stoichiometry="1"/>
          <addedPrefix:speciesReference species="LacI" stoichiometry="1"/>
        </addedPrefix:listOfReactants>
        <addedPrefix:listOfProducts>
          <addedPrefix:speciesReference species="C1" stoichiometry="1"/>
        </addedPrefix:listOfProducts>
        <addedPrefix:kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <minus/>
              <apply>
                <times/>
                <ci> kc_f </ci>
                <apply>
                  <power/>
                  <ci> IPTG </ci>
                  <ci> nc_IPTG </ci>
                </apply>
                <apply>
                  <power/>
                  <ci> LacI </ci>
                  <ci> nc_LacI </ci>
                </apply>
              </apply>
              <apply>
                <times/>
                <ci> kc_r </ci>
                <ci> C1 </ci>
              </apply>
            </apply>
          </math>
        </addedPrefix:kineticLaw>
      </addedPrefix:reaction>
      <addedPrefix:reaction metaid="iBioSim39" id="Complex_C2" reversible="true">
        <addedPrefix:listOfReactants>
          <addedPrefix:speciesReference species="aTc" stoichiometry="1"/>
          <addedPrefix:speciesReference species="TetR" stoichiometry="1"/>
        </addedPrefix:listOfReactants>
        <addedPrefix:listOfProducts>
