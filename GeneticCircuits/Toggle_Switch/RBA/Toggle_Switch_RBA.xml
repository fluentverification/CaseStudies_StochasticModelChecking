        <addedPrefix:kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <divide/>
              <apply>
                <times/>
                <apply>
                  <divide/>
                  <apply>
                    <times/>
                    <ci> P2 </ci>
                    <ci> ko </ci>
                    <ci> ko_f </ci>
                  </apply>
                  <ci> ko_r </ci>
                </apply>
                <ci> nr </ci>
              </apply>
              <apply>
                <plus/>
                <cn type="integer"> 1 </cn>
                <apply>
                  <times/>
                  <apply>
                    <divide/>
                    <ci> ko_f </ci>
                    <ci> ko_r </ci>
                  </apply>
                  <ci> nr </ci>
                </apply>
                <apply>
                  <power/>
