                      <divide/>
                      <ci> kr_f </ci>
                      <ci> kr_r </ci>
                    </apply>
                    <ci> TetR </ci>
                  </apply>
                  <ci> nc </ci>
                </apply>
              </apply>
            </apply>
          </math>
        </addedPrefix:kineticLaw>
      </addedPrefix:reaction>
