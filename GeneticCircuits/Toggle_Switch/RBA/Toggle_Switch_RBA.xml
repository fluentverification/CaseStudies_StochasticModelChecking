              <ci> kd </ci>
              <ci> C1 </ci>
            </apply>
          </math>
        </addedPrefix:kineticLaw>
      </addedPrefix:reaction>
      <addedPrefix:reaction metaid="iBioSim38" id="Complex_C1" reversible="true">
