        </addedPrefix:listOfProducts>
        <addedPrefix:kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <minus/>
              <apply>
                <times/>
                <ci> kc_f </ci>
                <apply>
                  <power/>
                  <ci> aTc </ci>
                  <ci> nc_aTc </ci>
                </apply>
                <apply>
                  <power/>
                  <ci> TetR </ci>
                  <ci> nc_TetR </ci>
                </apply>
              </apply>
              <apply>
                <times/>
                <ci> kc_r </ci>
                <ci> C2 </ci>
              </apply>
            </apply>
          </math>
        </addedPrefix:kineticLaw>
      </addedPrefix:reaction>
      <addedPrefix:reaction id="Production_P1" reversible="false">
        <addedPrefix:listOfProducts>
          <addedPrefix:speciesReference species="GFP" stoichiometry="10"/>
          <addedPrefix:speciesReference species="TetR" stoichiometry="10"/>
        </addedPrefix:listOfProducts>
        <addedPrefix:listOfModifiers>
          <addedPrefix:modifierSpeciesReference species="LacI"/>
          <addedPrefix:modifierSpeciesReference species="P1"/>
        </addedPrefix:listOfModifiers>
        <addedPrefix:kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <divide/>
              <apply>
                <times/>
                <apply>
                  <divide/>
                  <apply>
                    <times/>
                    <ci> P1 </ci>
                    <ci> ko </ci>
                    <ci> ko_f </ci>
                  </apply>
                  <ci> ko_r </ci>
                </apply>
                <ci> nr </ci>
              </apply>
              <apply>
                <plus/>
                <cn type="integer"> 1 </cn>
                <apply>
                  <times/>
                  <apply>
                    <divide/>
                    <ci> ko_f </ci>
                    <ci> ko_r </ci>
                  </apply>
                  <ci> nr </ci>
                </apply>
                <apply>
                  <power/>
                  <apply>
                    <times/>
                    <apply>
                      <divide/>
                      <ci> kr_f </ci>
                      <ci> kr_r </ci>
                    </apply>
                    <ci> LacI </ci>
                  </apply>
                  <ci> nc </ci>
                </apply>
              </apply>
            </apply>
          </math>
        </addedPrefix:kineticLaw>
      </addedPrefix:reaction>
      <addedPrefix:reaction id="Production_P2" reversible="false">
        <addedPrefix:listOfProducts>
          <addedPrefix:speciesReference species="LacI" stoichiometry="10"/>
