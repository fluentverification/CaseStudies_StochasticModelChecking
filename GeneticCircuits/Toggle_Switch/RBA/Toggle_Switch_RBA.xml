    <addedPrefix:listOfReactions>
      <addedPrefix:reaction metaid="iBioSim33" id="Degradation_TetR" reversible="false">
        <addedPrefix:listOfReactants>
          <addedPrefix:speciesReference species="TetR" stoichiometry="1"/>
        </addedPrefix:listOfReactants>
        <addedPrefix:kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> kd </ci>
              <ci> TetR </ci>
            </apply>
          </math>
        </addedPrefix:kineticLaw>
      </addedPrefix:reaction>
      <addedPrefix:reaction metaid="iBioSim34" id="Degradation_GFP" reversible="false">
        <addedPrefix:listOfReactants>
